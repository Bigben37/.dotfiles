! fonts
URxvt.font:  xft:DejaVu Sans Mono:pixelsize=13,\
             xft:DejaVu Sans Mono for Powerline:pixelsize=13
URxvt.boldFont: xft:DejaVu Sans Mono:pixelsize=13:bold
URxvt.italicFont: xft:DejaVu Sans Mono:pixelsize=13:italic
URxvt.boldItalicFont: xft:DejaVu Sans Mono:pixelsize=13:bold:italic
URxvt.letterSpace: -1

!following settings for fonts, works with Infinality
Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

! cursor
Xcursor.theme: Adwaita
Xcursor.size:  0

! basic stuff 
URxvt.depth: 32
URxvt*scrollBar:                      false
URxvt*mouseWheelScrollPage:           false
URxvt*cursorBlink:                    true
URxvt*saveLines:                      5000
URxvt.iso14755:         False
URxvt.internalBorder:      0
URxvt.externalBorder:      0

!color
! Base16 shapeshifter
! Scheme: Tyler Benziger (http://tybenz.com)

URxvt*background:                     [85]#000000
URxvt*foreground:                     #ababab
URxvt*cursorColor:                    #ababab

URxvt*scrollColor:                    #ababab
